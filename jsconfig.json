{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "paths": {
      "@/*": ["./src/*"],
      "react": ["./node_modules/preact/compat/"],
      "react-dom": ["./node_modules/preact/compat/"]
    },
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "exclude": ["node_modules"],
  "include": ["src"]
}
